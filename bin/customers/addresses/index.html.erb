<main>
  <p id="notice"></p>
  <div class="container">
    <div class="row">
      <div class="col">
        <h5>配送先登録／一覧</h5>
        <%= form_with model: @genres, url:admin_genres_path, local:true do |f| %>
        <div class="form-group">
          <label for="postal_code">郵便番号(ハイフンなし)</label>
          <%= f.text_field :postal_code %>
        <div class="form-group">
          <label for="address">住所</label>
          <%= f.text_field :address %>
        <div class="form-group">
          <label for="name">宛名</label>
          <%= f.text_field :name %>
          <%= f.submit "新規登録", class: "btn btn-success" %>
        <div class="p-3"></div>
        <thead>
          <table border="1">
            <tr>
              <td>郵便番号</td> 
              <td>住所</td>
              <td>宛名</td>
              <td>　　　</td>
            </tr>
            <tr>
              <td></td>
              <td></td>
              <td></td>
              <td><%= link_to "編集する", new_admin_session_path, class: "btn btn-success" %>
              <%= link_to "削除する", new_admin_session_path, class: "btn btn-danger" %>
              </td>
            </tr>
          </table>
        </thead>
        </div>
        </div>
        </div>
      </div>
    </div>
  </div>
</main>
<% end %>