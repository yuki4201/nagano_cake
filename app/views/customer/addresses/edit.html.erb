<main>
  <p id="notice"></p>
  <div class="container">
    <div class="row">
      <div class="col">
        <h5>配送先編集</h5>
        <%= form_with model: @genres, url:admin_genres_path, local:true do |f| %>
        <div class="form-group">
          <label for="postal_code">郵便番号(ハイフンなし)</label>
          <%= f.text_field :postal_code %>
        <div class="form-group">
          <label for="address">住所</label>
          <%= f.text_field :address %>
        <div class="form-group">
          <label for="name">宛名</label>
          <%= f.text_field :name %>
          <%= f.submit "変更内容を保存", class: "btn btn-success" %>
        </div>
        </div>
        </div>
      </div>
    </div>
  </div>
</main>
<% end %>
